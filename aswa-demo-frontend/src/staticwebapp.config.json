{
	"navigationFallback": {
	  "rewrite": "index.html"
	},
	"routes": [
		{
			"route": "/login",
			"methods": ["GET"],
			"redirect": "/.auth/login/aad"
		},
		{
			"route": "/RFC2324",
			"statusCode": 418
		},
		{
			"route": "/*",
			"allowedRoles": ["authenticated"]
		}
	],
	"responseOverrides": {
		"401": {
			"redirect": "/login",
			"statusCode": 200
		}
	}
}